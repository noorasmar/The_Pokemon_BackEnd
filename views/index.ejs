                        accusamus pariatur, similique enim nulla voluptatibus iste tenetur quis.
                    </p>
                </div>
            </div>
        </div>
    
    </div>
    <!-- End About Us -->

    <!-- Start List Box -->
    <div class="list-box" id="list-pokemon">
        <div class="container">
            <div class="title">
                <h2>
                    List Pokémon Pokédex
                </h2>
            </div>
            <div class="search-box">
                <div class="row">
                    <div class="input-field col s12 m12 l6">
                        <input type="text" name="search" id="search" placeholder="Enter The Pokemon Name">
                    </div>
                    <div class="input-field col s12 m6 l4">
                        <button class="btn waves-effect btn-search" type="submit" name="action"
                            onclick="searchPokemon()">
                            <i class="material-icons center">search</i>
                        </button>
                    </div>
                    <div class="input-field col s12 m6 l2">
                        <a class="btn-reset" onclick="restoreData()">
                            Restore Data
                        </a>
                    </div>
                </div>
            </div>
            <div class="list-data">
                <div class="error-search">
                    <div class="alert">
                        <h3>
                            Oops !! Not Found
                        </h3>
                        <p>
                            you insert the wrong name of pokemon.
                        </p>
                    </div>
                </div>
                <div class="row row-data">

                </div>
            </div>
        </div>
    </div>
    <!-- End List Box -->

    <!-- Start List Captured -->
    <div class="list-captured" id="list-captured">
        <div class="container">
            <div class="title">
                <h2>
                    List Pokémon Captured
                </h2>
            </div>
            <div class="row row-captured">

            </div>
        </div>
    </div>
    <!-- End List Captured -->

    <!-- Start Our Achivement -->
    <div class="our-achivement" id="achivement">
        <div class="container">
            <div class="title">
                <h2>
                    Our Achivement
                </h2>
            </div>
            <div class="row">
                <div class="col s12 m6 l3">
                    <div class="item">
                        <span class="count" data-from="0" data-to="1118"  data-speed="2000" data-refresh-interval="50">

                        </span>
                        <span>
                            Num Pokemon
                        </span>
                    </div>
                </div>
                <div class="col s12 m6 l3">
                    <div class="item">
                        <span class="count" data-from="0" data-to="5000" data-speed="2000" data-refresh-interval="50">

                        </span>
                        <span>
                            Enrolled
                        </span>
                    </div>
                </div>
                <div class="col s12 m6 l3">
                    <div class="item">
                        <span class="count" data-from="0" data-to="50000"  data-speed="2000" data-refresh-interval="50">

                        </span>
                        <span>
                            Earn Money
                        </span>
                    </div>
                </div>
                <div class="col s12 m6 l3">
                    <div class="item">
                        <span class="count" data-from="0" data-to="100"  data-speed="2000" data-refresh-interval="50">

                        </span>
                        <span>
                            Satisfaction Rate
                        </span>
                    </div>
                </div>
            </div>

        </div>
    </div>
    <!-- End Our Achivement -->

